REQUEST POST "https://api.sendgrid.com/v3/public/tokens" 
  CONTENT "{\"username\":\"<USER>\",\"password\":\"<PASS>\"}" 
  CONTENTTYPE "application/json" 
  HEADER "Accept: */*" 
  HEADER "Accept-Encoding: gzip, deflate, br" 
  HEADER "Accept-Language: fr-FR,fr;q=0.9,en-US;q=0.8,en;q=0.7" 
  HEADER "Authorization: token undefined" 
  HEADER "Connection: keep-alive" 
  HEADER "Content-Length: 47" 
  HEADER "Content-Type: application/json" 
  HEADER "Host: api.sendgrid.com" 
  HEADER "Origin: https://app.sendgrid.com" 
  HEADER "Sec-Fetch-Dest: empty" 
  HEADER "Sec-Fetch-Mode: cors" 
  HEADER "Sec-Fetch-Site: same-site" 
  HEADER "User-Agent: Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/81.0.4044.138 Safari/537.36" 

KEYCHECK 
  KEYCHAIN Failure OR 
    KEY "authorization required" 
    KEY "errors" 
    KEY "Your username or password is invalid" 
  KEYCHAIN Success OR 
    KEY "token" 

